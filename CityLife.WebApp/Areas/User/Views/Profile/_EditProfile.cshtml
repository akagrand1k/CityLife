<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h5 class="modal-title" id="myModalLabel">Редактирование профиля</h5>
</div>
<div class="modal-body">
    <!-- Row -->
    <div class="row">
        <div class="col-lg-12">
            <div class="">
                <div class="panel-wrapper collapse in">
                    <div class="panel-body pa-0">
                        <div class="col-sm-12 col-xs-12">
                            <div class="form-wrap">
                                <form role="form" id="profile-emailchange">
                                    <div class="form-body overflow-hide">

                                        <div class="form-group">
                                            <label class="control-label mb-10" for="FamilyName">Фамилия</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="icon-user"></i></div>
                                                <input type="text" class="form-control" id="FamilyName" placeholder="Иванов">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label mb-10" for="GivenName">Имя</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="icon-user"></i></div>
                                                <input type="text" class="form-control" id="GivenName" placeholder="Иванов">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label mb-10" for="FirstPhone">Телефон</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="icon-phone"></i></div>
                                                <input type="text" class="form-control" id="FirstPhone" placeholder="+7(999)999-99-99">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label mb-10" for="SecondPhone">Дополнительный телефон</label>
                                            <div class="input-group">
                                                <div class="input-group-addon"><i class="icon-phone"></i></div>
                                                <input type="text" class="form-control" id="SecondPhone" placeholder="8(84457)9-99-99">
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label class="control-label mb-10" for="BrithDate">Дата рождения</label>
                                            <div class="input-group date" id="birthdayDatePicker">
                                                <div class="input-group-addon"><i class="fa fa-calendar"></i></div>
                                                <input type="text" class="form-control" id="BrithDate" placeholder="10.10.2010">
                                            </div>
                                        </div>

                                    </div>
                                    <div class="modal-footer">
                                        <button type="submit" class="btn btn-success waves-effect" id="saveProfile" data-dismiss="modal">Сохранить</button>
                                        <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">Отменить</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

